{
  "name": "movie-graphql",
  "version": "1.0.0",
  "description": "Movie API with GraphQL",
  "main": "server.js",
  "scripts": {
    "postinstall": "npm install express babel-cli",
    "start": "babel-node server.js",
    "server": "nodemon --exec babel-node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9min/movie-graphql.git"
  },
  "author": "9min",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/9min/movie-graphql/issues"
  },
  "homepage": "https://github.com/9min/movie-graphql#readme",
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "axios": "^0.21.1",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "nodemon": "^1.18.9"
  }
}
